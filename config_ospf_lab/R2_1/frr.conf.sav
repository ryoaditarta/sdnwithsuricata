frr version 8.1
frr defaults traditional
hostname R2_1
service integrated-vtysh-config
!
interface eth0
 ip address 10.12.1.2/24
 ip ospf area 0
exit
!
interface eth1
 ip address 172.17.1.1/24
 ip ospf area 0
exit
!
interface lo
 ip address 10.152.1.1/24
exit
!
router ospf
 ospf router-id 10.152.1.1
exit
!
